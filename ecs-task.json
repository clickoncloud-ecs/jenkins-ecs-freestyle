{
    "family": "mynodeapp",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "mynodeapp",
            "image": "111836646603.dkr.ecr.us-east-1.amazonaws.com/my-repo:v_%BUILD_NUMBER%",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "hostPort": 4000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
